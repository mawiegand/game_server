<h1>Listing effect_resource_effects</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Origin</th>
    <th>Resource Pool</th>
    <th>Bonus</th>
    <th>Resource</th>
    <th>Finished at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @effect_resource_effects.each do |effect_resource_effect| %>
  <tr>
    <td><%= effect_resource_effect.type_id %></td>
    <td><%= effect_resource_effect.origin_id %></td>
    <td><%= effect_resource_effect.resource_pool_id %></td>
    <td><%= effect_resource_effect.bonus %></td>
    <td><%= effect_resource_effect.resource_id %></td>
    <td><%= effect_resource_effect.finished_at %></td>
    <td><%= link_to 'Show', effect_resource_effect %></td>
    <td><%= link_to 'Edit', edit_effect_resource_effect_path(effect_resource_effect) %></td>
    <td><%= link_to 'Destroy', effect_resource_effect, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

