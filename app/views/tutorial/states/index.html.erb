<h1>Listing tutorial_states</h1>

<table>
  <tr>
    <th>Character</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tutorial_states.each do |tutorial_state| %>
  <tr>
    <td><%= tutorial_state.character_id %></td>
    <td><%= link_to 'Show', tutorial_state %></td>
    <td><%= link_to 'Edit', edit_tutorial_state_path(tutorial_state) %></td>
    <td><%= link_to 'Destroy', tutorial_state, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New State', new_tutorial_state_path %>
